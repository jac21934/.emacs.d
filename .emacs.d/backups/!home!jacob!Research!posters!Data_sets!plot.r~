delim = ","  # or is it "\t" ?
dec = "."    # or is it "," ?

dat <- read.table("rat_cortex.csv", header = TRUE,stringsAsFactors=FALSE,sep=delim, dec=dec)

length(dat$Electrode)

dat$seconds
as.numeric(as.character(dat$seconds))



setEPS()
postscript("rat_cortex.eps")


plot(as.numeric(dat$seconds), as.numeric(dat$Electrode),pch=19,	xlab="", ylab="", main="", col="red" )



dat <- read.table("salamander.csv", header = TRUE,stringsAsFactors=FALSE,sep=delim, dec=dec)

length(dat$Electrode)

dat$seconds
