digraph example_neural_network {
	rankdir=LR;
	size="8,5"
	
  n1[shape=circle,style=filled, fillcolor="#407fbb", label=<n<SUB>1</SUB>>];   
  n2[shape=circle,style=filled, fillcolor="#407fbb", label=<n<SUB>2</SUB>>];
 
  n3[shape=circle,style=filled, fillcolor="#407fbb", label=<n<SUB>3</SUB>>];   
  n4[shape=circle,style=filled, fillcolor="#407fbb", label=<n<SUB>4</SUB>>];

	n5[shape=circle,style=filled, fillcolor="#407fbb", label=<n<SUB>5</SUB>>];   
  n6[shape=circle,style=filled, fillcolor="#407fbb", label=<n<SUB>6</SUB>>];


	n1 -> n2;
	n1 -> n3;
	n1 -> n4;
	
	
	
	n2 -> n3;

	n4 -> n5;
	n4 -> n1;
	
	
	

}
